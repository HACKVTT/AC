	<section class="covercontent">
		<div class="container-fluid bg-white">
			<div class="row">
				{{> sidebar}}
				<div class="col-md-6">
					<div class="row">
						<div class="maincontent">
							<ol class="breadcrumb">
							    <li class="active"><a href="#">Trang Chủ</a></li>
							</ol>

							<div class="container home-content">
								{{#each data.post}}
									{{#ifeq @index 0}}
										<div class="block-item-news">
											<h2 class="block-item-news__title mb1">
												<a href="/{{cateservice.slug}}/{{slug}}.html">{{title}}</a>
											</h2>
											<div class="block-item-news__content">
												<a href="">
													<img src="/images/{{image.filename}}" alt="project-detail-photo" class="img-fluid center-block mb1">
												</a>

												<div class="block-item-news__brief">
													{{{content.brief}}}
												</div>
											</div><!-- end content-news-item -->
										</div> <!--end block-item-horizontal -->
									{{else}}
										<div class="block-item-news">
											<h2 class="block-item-news__title">
												<a href="/{{cateservice.slug}}/{{slug}}.html">{{title}}</a>
											</h2>
											<div class="block-item-news__content">
												<a href="" class="block-item-news__img-thumb">
													{{#if image.filename}}
														<img src="/images/{{image.filename}}">
													{{else}}
														<img src="/images/no-image.jpg" alt="no-image">
													{{/if}}
												</a>

												<div class="block-item-news__brief">
													{{{content.brief}}}
												</div>
											</div><!-- end content-news-item -->
										</div> <!--end block-item-horizontal -->
									{{/ifeq}}
								{{/each}}

								<p>
									<strong>Tên Công Ty</strong>: Công Ty TNHH tư vấn dịch vụ kế toán A&C Ninh Thuận <br>
									<strong>Địa chỉ</strong>: 25 Phú Xuân Thưởng, Phường Tấn Tài, TP. Phan Rang - Tháp Chàm, Tỉnh Ninh Thuận <br>
									<strong>Điện Thoại</strong>: 091 7659 475 - 091 3938 090 <br>
									<strong>Email</strong>: ketoanac@gmail.com
								</p>	
							</div> <!-- end home-content -->
						</div><!-- end maincontent -->

					</div> <!-- end row -->
				</div><!-- end col-md-6 -->

				{{> sidebarright}}
			</div> <!-- end row -->
		</div> <!-- end container-fluid -->
	</section><!-- end covercontent -->